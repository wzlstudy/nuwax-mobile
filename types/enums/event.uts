/**
 * 事件类型枚举
 * 用于 batch 轮询事件分发
 * 
 * 与 PC 端 @/types/enums/event.ts 保持一致
 */
export enum EventTypeEnum {
  // 有新的通知消息
  NewNotifyMessage = 'new_notify_message',
  // 会话消息列表需要刷新
  RefreshChatMessage = 'refresh_chat_message',
  // 文件列表需要刷新
  RefreshFileList = 'refresh_file_list',
  // 会话结束后，更新会话状态
  ChatFinished = 'chat_finished',
}
